{
  "name": "my-ts-app-client",
  "version": "1.0.0",
  "description": "Client application for the TypeScript server-client project.",
  "main": "src/index.js",
  "scripts": {
    "start": "concurrently \"cd ../server && npm run start\" \"npm run start:prod\"",
    "start:dev": "concurrently \"cd ../server && npm run start\" \"npm run start:app\"",
    "start:app": "webpack-cli serve --mode=development --env development",
    "start:prod": "node dist/server.js",
    "package": "copyfiles -E -u 3 \"../server/dist/**/*\" dist",
    "build": "npm run clean && concurrently --kill-others-on-fail \"tsc\" \"webpack --mode=production --env production\"",
    "dev": "webpack serve --mode development",
    "clean": "rimraf dist",
    "prettier": "prettier --no-error-on-unmatched-pattern --write --config ./.prettierrc --ignore-path=./.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "prettier:check": "prettier --no-error-on-unmatched-pattern --check --config ./.prettierrc --ignore-path=./.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@azure/communication-calling": "^1.34.1",
    "@azure/communication-calling-effects": "^1.1.2",
    "@azure/communication-chat": "^1.5.4",
    "@azure/communication-identity": "^1.0.0",
    "@azure/communication-react": "1.26.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@fluentui/react": "^8.120.0",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "css-loader": "~7.1.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.6.0",
    "rimraf": "^2.6.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^4.4.3",
    "typescript-eslint": "^8.36.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
